{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["ES2015"],
    "outDir": "./dist",
    "noEmitOnError": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ]
}
